{
    "name": "sveltekit-demo",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "pretest": "npx scrypt-cli compile",
        "build:contract": "npx scrypt-cli@latest compile",
        "deploy:contract": "npx tsx ./scripts/deploy.ts",
        "verify:contract": "npx scrypt-cli verify $(cat .scriptHash) ./src/contracts/sveltekitDemo.ts",
        "genprivkey": "npx tsx ./scripts/privateKey.ts"
    },
    "devDependencies": {
        "@sveltejs/adapter-auto": "^3.0.0",
        "@sveltejs/kit": "^2.5.18",
        "@sveltejs/vite-plugin-svelte": "^3.0.0",
        "autoprefixer": "^10.4.19",
        "dotenv": "^16.4.5",
        "lucia": "^3.2.0",
        "postcss": "^8.4.39",
        "svelte": "^4.2.7",
        "svelte-check": "^3.6.0",
        "tailwindcss": "^3.4.4",
        "tslib": "^2.4.1",
        "typescript": "^5.0.0",
        "vite": "^5.0.3",
        "vite-plugin-node-polyfills": "^0.22.0"
    },
    "type": "module",
    "dependencies": {
        "@bsv/sdk": "^1.1.12",
        "@sveltejs/adapter-vercel": "^5.4.1",
        "@vercel/postgres": "^0.9.0",
        "neucron-sdk": "^0.2.3",
        "neucron-signer": "^1.3.2",
        "scrypt-ts": "^1.3.31"
    },
    "description": "Everything you need to build a Svelte project, powered by [`create-svelte`](https://github.com/sveltejs/kit/tree/main/packages/create-svelte).",
    "main": "postcss.config.js",
    "author": "",
    "license": "ISC"
}